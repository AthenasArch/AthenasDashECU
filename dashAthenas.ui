      <property name="styleSheet">
       <string notr="true">QComboBox {
	gridline-color: rgb(255, 255, 255);
    background-color: #282a36;
    border: 2px solid #6272a4;
    border-radius: 8px;
    color: #f8f8f2;
    font-family: Verdana;
    font-size: 18px;
    padding: 6px;
}

QComboBox:hover {
    border-color: #ffffff;
}

QComboBox:editable {
    background-color: #44475a;
}

QComboBox:!editable, QComboBox::drop-down:editable {
    background: #282a36;
}

QComboBox:!editable:on, QComboBox::drop-down:editable:on {
    background: #282a36;
}

QComboBox:on { /* quando selecionado */
    border-color: #f1fa8c;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left-width: 1px;
    border-left-color: #6272a4;
    border-left-style: solid; /* estilo de linha da esquerda - pode ser qualquer um dos seguintes: solid, dashed, dot-dash, dot-dot-dash, none */
    border-top-right-radius: 3px; /* raio superior direito da borda quando a caixa não está focada */
    border-bottom-right-radius: 3px; /* raio inferior direito da borda quando a caixa não está focada */
}

QComboBox::down-arrow {
    image: url(/path/to/your/image.png); /* substitua '/path/to/your/image.png' pelo caminho correto para sua imagem */
}

QComboBox::down-arrow:on { /* mude a cor do botão para laranja quando a caixa está focada */
    top: 1px;
    left: 1px;
}

QComboBox QAbstractItemView {
    color: #f8f8f2; /* cor do texto dos itens */
    background-color: #3c0a8c; /* cor de fundo dos itens */
}


